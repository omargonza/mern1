{
    "version": 2,
    "name": "mern-electricos",
    "builds": [
        { "src": "src/app.js", "use": "@vercel/node" }
    ],
    "routes": [
        { "src": "/api/auth/(.*)", "dest": "/src/app.js" },
        { "src": "/api/(.*)", "dest": "/src/app.js" },
        { "src": "/(.*)", "dest": "/src/app.js" }
    ],
    "env": {
        "FRONTEND_URL": "http://localhost:5173"
        
    }
}
